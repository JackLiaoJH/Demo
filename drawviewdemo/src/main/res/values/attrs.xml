<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LabelView">
        <attr name="text" format="string"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="textSize" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="ProgressWheel">
        <attr name="pw_text" format="string" />
        <attr name="pw_textColor" format="color" />
        <attr name="pw_textSize" format="dimension" />
        <attr name="pw_barColor" format="color" />
        <attr name="rimColor" format="color" />
        <attr name="rimWidth" format="dimension" />
        <attr name="spinSpeed" format="dimension" />
        <attr name="delayMillis" format="integer" />
        <attr name="circleColor" format="color" />
        <attr name="radius" format="dimension" />
        <attr name="pw_barWidth" format="dimension" />
        <attr name="pw_barLength" format="dimension" />
        <attr name="contourColor" format="color"/>
        <attr name="contourSize" format="dimension"/>
    </declare-styleable>


</resources>